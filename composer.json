     "keywords": ["noiselabs", "kayzore", "configparser", "config", "parser"],
    "homepage": "https://github.com/kayzore/configparser.php",
    "version": "0.1.0",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vítor Brandão",
            "email": "noisebleed@noiselabs.org"
        },
        {
            "name": "Jerome Lelievre",
            "email": "jerome.lelievre2@gmail.com"
        }
    ],
